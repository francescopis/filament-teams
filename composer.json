{
    "name": "francescopis/filament-teams",
    "description": "Team and User associations package for Filament.",
    "keywords": [
        "Jeff Greco",
        "laravel",
        "Filament",
        "filament-teams"
    ],
    "homepage": "https://github.com/jeffgreco13/filament-teams",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Greco",
            "email": "jeff@jeffpgreco.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9",
        "filament/filament": "^2.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "JeffGreco13\\FilamentTeams\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeffGreco13\\FilamentTeams\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeffGreco13\\FilamentTeams\\FilamentTeamsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
